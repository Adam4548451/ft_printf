START
    int i
    int x
    char *pt;
    char *output

    i = count_character(string, '%')
    x = 0
    pt = string;
    output = NULL;
    WHILE (x < i)
        output = dupCatResize(output, pt, ft_strchr(pt, (int)'%')
        pt = Find '%' in string
        pt++;
        IF (isExigence1(*pt))
            IF (ft_isspace((int)pt[1]) || !pt[1])
                Do something depending on value of pt
                Concatenate the returned string to output
            ELSE
                ERROR
        ELSEIF (isExigence2(*pt))
        ELSE 
            BREAK
        ENDIF
        x++;
    ENDWHILE
    Return the output
END

